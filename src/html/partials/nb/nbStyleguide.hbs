{{/content}}

{{#> nbForm}}
	{{#*inline "form"}}

    <h2>Form Styles</h2>

	<form id="nb-form-{{page.name}}" class="uk-form-stacked InputfieldForm" method="post" data-uk-nb-form novalidate data-colspacing="1">

		{{#> ukGrid class="uk-grid-small"}}
			{{#*inline "cells"}}

			{{> nbInput class="uk-width-5-6@m" label="Your name" name="name" title="Please enter your name"}}

			{{> nbInput class="uk-width-5-6@m" label="Email address" name="email" type="email" title="Please enter a valid email address"}}

			{{> nbInput class="uk-width-3-5@m" label="Telephone" name="tel" type="tel"}}

			{{> nbInput label="Your Enquiry" name="enquiry" type="textarea" title="Please enter your enquiry"}}

			{{> nbInput name="submit" type="submit" value="Send a message"}}

			{{/inline}}
		{{/ukGrid}}

	</form>

	{{/inline}}
{{/nbForm}}

